@{
    ViewData["Title"] = "Cirify";
}
<section class="box">
    <h1>Tu biblioteca musical</h1>
    @foreach(KeyValuePair<int, Disco> item in ViewBag.discos)
    {
        <div>
        <h3><a href='@Url.Action("SelectDisco", "Home", new {id = @item.Value.id})'> <img src = "/img/@(item.Value.nombreDisco).jpg" alt ="imagen disco"/></a></h3>
        </div>
    }

</section>
